<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="diary.bo.UserBO" table="USER">
    <id name="userId">
      <column name="USER_ID"/>
      <generator class="identity"/>
    </id>
    <property name="name">
      <column name="NAME"/>
    </property>
    <property name="username">
      <column name="USERNAME"/>
    </property>
    <property name="birthday">
      <column name="BIRTHDAY"/>
    </property>
    <property name="avartar">
      <column name="AVARTAR"/>
    </property>
    <property name="address">
      <column name="ADDRESS"/>
    </property>
    <property name="phone">
      <column name="PHONE"/>
    </property>
    <property name="email">
      <column name="EMAIL"/>
    </property>
    <property name="gender">
      <column name="GENDER"/>
    </property>
    <property name="password">
      <column name="PASSWORD"/>
    </property>
    <property name="active">
      <column name="ACTIVE"/>
    </property>
  </class>
</hibernate-mapping>
