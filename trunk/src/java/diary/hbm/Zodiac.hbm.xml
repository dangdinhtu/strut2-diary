<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="diary.bo.ZodiacBO" table="ZODIAC">
          <id name="zodiacId">
            <column name="ZODIAC_ID"/>
            <generator class="identity"/>
          </id>
          <property name="name">
            <column name="NAME"/>
          </property>
          <property name="date">
            <column name="DATE"/>
          </property>
          <property name="image">
            <column name="IMAGES"/>
          </property>
    </class>
</hibernate-mapping>